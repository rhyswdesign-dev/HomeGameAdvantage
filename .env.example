# Firebase Configuration
# Get these values from your Firebase project settings
EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key-here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=G-ABCDEF123

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_PROVIDER=memory
EXPO_PUBLIC_POSTHOG_API_KEY=your-posthog-api-key
EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com
EXPO_PUBLIC_SEGMENT_WRITE_KEY=your-segment-write-key

# Monetization Configuration
EXPO_PUBLIC_ADMOB_APP_ID=ca-app-pub-your-app-id
EXPO_PUBLIC_ADMOB_LIFE_REWARD_ID=ca-app-pub-your-life-reward-id
EXPO_PUBLIC_ADMOB_XP_BONUS_ID=ca-app-pub-your-xp-bonus-id
EXPO_PUBLIC_APPLOVIN_SDK_KEY=your-applovin-sdk-key

# Development Settings
EXPO_PUBLIC_USE_FIREBASE_EMULATOR=false
NODE_ENV=development